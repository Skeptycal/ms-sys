#: src/main.c:115
#, c-format
msgid ""
"Usage:\n"
"\t%s [options] [device]\n"
"Options:\n"
msgstr ""
"Användning:\n"
"\t%s [flaggor] [enhet]\n"
"Flaggor:\n"

#: src/main.c:117
msgid "    -1, --fat12     Write a FAT12 floppy boot record to device\n"
msgstr "    -1, --fat12     Skriv en FAT12 diskett boot record till enheten\n"

#: src/main.c:119
msgid "    -3, --fat32     Write a FAT32 partition boot record to device\n"
msgstr "    -3, --fat32     Skriv en FAT32 partition boot record till enheten\n"

#: src/main.c:120
msgid "    -f, --force     Force writing of boot record\n"
msgstr "    -f, --force     Tvinga skrivande av boot record\n"

#: src/main.c:121
msgid "    -h, --help      Display this help and exit\n"
msgstr "    -h, --help      Visa denna hjälp och avsluta\n"

#: src/main.c:122
msgid "    -m, --mbr       Write a master boot record to device\n"
msgstr "    -m, --mbr       Skriv en master boot record till enheten\n"

#: src/main.c:123
msgid "    -v, --version   Show program version\n"
msgstr "    -v, --version   Visa versionsinformation\n"

#: src/main.c:125
msgid ""
"    -w, --write     Write automaticly selected boot record to device\n"
"\n"
msgstr ""
"    -w, --write     Skriv automatiskt vald boot record till enheten\n"
"\n"

